<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="speex" />
		<Option pch_mode="2" />
		<Option compiler="armelfgcc" />
		<Build>
			<Target title="release">
				<Option output="libspeex-jni.so" imp_lib="$(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).a" def_file="$(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).def" prefix_auto="0" extension_auto="0" />
				<Option object_output="." />
				<Option type="3" />
				<Option compiler="armelfgcc" />
				<Compiler>
					<Add option="-mthumb" />
					<Add option="-mthumb-interwork" />
					<Add option="-msoft-float" />
					<Add option="-fomit-frame-pointer" />
					<Add option="-O2" />
					<Add option="-Wall" />
					<Add option="-DANDROID" />
					<Add option="-D__ARM_ARCH_5__" />
					<Add option="-D__ARM_ARCH_5T__" />
					<Add option="-D__ARM_ARCH_5E__" />
					<Add option="-D__ARM_ARCH_5TE__" />
					<Add option="-DNDEBUG" />
					<Add directory="../../include" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add library="log" />
				</Linker>
			</Target>
			<Target title="windbg">
				<Option output="testr.exe" prefix_auto="0" extension_auto="0" />
				<Option object_output="." />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option parameters="l test.rar" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
				<Linker>
					<Add library="libunrar.a" />
					<Add directory="." />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add directory="../.." />
		</Compiler>
		<Unit filename="speex-jni.cpp" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
